<h1>Listing Social Bookmarks</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @soc_books.each do |soc_book| %>
      <tr>
        <td><%= link_to soc_book.url, soc_book.url %></td>
        <td><%= link_to 'Edit', edit_soc_book_path(soc_book) %>
        <%= link_to 'Destroy', soc_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
